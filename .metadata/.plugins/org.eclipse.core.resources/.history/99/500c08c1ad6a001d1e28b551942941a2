package com.perscholas.java_basics;

public class HomeClass {

	public static void main(String[] args) {
		
		int num1 = 3;
		int num2 = 4 ;
		int sum = num1 + num2 ;
	
		System.out.println("the sum two int values of  num1 and num2 = "+ sum);
		
		
		double  num3 = 3.4;
		double  num4 = 4.2 ;
		double  doubleSum = num3 + num4 ;
	
		System.out.println("the sum of double values of  num3 and num4 = "+ doubleSum);
		
		
		int  num5 = 3;
		double  num6 = 4.22 ;
		double  Sum3 = num3 + num4 ;
	
		System.out.println("the sum of double and int  values of  num5 and num6 = "+ Sum3);
		
		
	}

}
